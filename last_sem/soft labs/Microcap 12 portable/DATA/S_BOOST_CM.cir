.lib smps_cb.lib
**** Generic PWM controller parameters ****
.PARAM REF = 5V				; internal reference voltage
.PARAM PERIOD = 5U			; switching period
.PARAM RATIO = 1				; Max peak current versus err. amp level
.PARAM DUTYMAX = 0.8			; maximum duty cycle
.PARAM RAMP = 5V				; external ramp for compensation
.PARAM VOUTHI = 15V			; driver output voltage high
.PARAM VOUTLO = 100M			; driver output voltage low
.PARAM ROUT = 10				; driver output resistor
**** Internal error amplifier parameters ****
.PARAM VHIGH = 8			; maximum output voltage
.PARAM ISINK = 15M			; Sink capability
.PARAM ISOURCE = 500U		; Source capability
.PARAM VLOW = 100M			; Minimum output voltage
.PARAM POLE = 30				; First pole in Hertz
.PARAM GAIN = 31622			; DC open-loop gain (default=90dB)

.SUBCKT IRF7413 1 2 3
* External Node Designations
* Node 1 -> Drain
* Node 2 -> Gate
* Node 3 -> Source
M1 9 7 8 8 MM L=100u W=100u
* Default values used in MM:
* The voltage-dependent capacitances are
* not included. Other default values are:
*   RS=0 RD=0 LD=0 CBD=0 CBS=0 CGBO=0
.MODEL MM NMOS LEVEL=1 IS=1e-32
+ VTO=2.66432 LAMBDA=0.0048933 KP=89.0396
+ CGSO=1.68868e-05 CGDO=1.33184e-06
RS 8 3 0.00853462
D1 3 1 MD
.MODEL MD D IS=7.69994e-08 RS=0.0402311 N=1.5 BV=30
+ IBV=0.00025 EG=1.06591 XTI=3.99512 TT=1.99941e-05
+ CJO=2.25643e-09 VJ=0.5 M=0.420315 FC=0.1
RDS 3 1 2.4e+07
RD 9 1 0.0001
RG 2 7 6.86824
D2 4 5 MD1
* Default values used in MD1:
*   RS=0 EG=1.11 XTI=3.0 TT=0
*   BV=infinite IBV=1mA
.MODEL MD1 D IS=1e-32 N=50
+ CJO=1.28e-09 VJ=2.11133 M=0.9 FC=1e-08
D3 0 5 MD2
* Default values used in MD2:
*   EG=1.11 XTI=3.0 TT=0 CJO=0
*   BV=infinite IBV=1mA
.MODEL MD2 D IS=1e-10 N=0.4 RS=3.00001e-06
RL 5 10 1
FI2 7 9 VFI2 -1
VFI2 4 0 0
EV16 10 0 9 7 1
CAP 11 10 1.28e-09
FI1 7 9 VFI1 -1
VFI1 11 6 0
RCAP 6 10 1
D4 0 6 MD3
* Default values used in MD3:
*   EG=1.11 XTI=3.0 TT=0 CJO=0
*   RS=0 BV=infinite IBV=1mA
.MODEL MD3 D IS=1e-10 N=0.4
.ENDS

*** D:\MC6\DATA\DIODE.LBR
*** Schottky Barrier Rectifier
.MODEL 1N5820 D (IS=2.78088U N=872.811M BV=20 RS=50.7957M CJO=537.195P
+ VJ=15.0222 M=1 EG=690M XTI=2 RL=51.8006K)


